{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "wang-file-searcher",
  "version": "0.1.0",
  "identifier": "com.wang-file-searcher.app",
  "build": {
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:14200",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/wang-file-searcher/browser"
  },
  "app": {
    "windows": [
      {
        "title": "文件搜索工具",
        "width": 1200,
        "height": 800,
        "minWidth": 900,
        "minHeight": 600,
        "x": 0,
        "y": 0
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
